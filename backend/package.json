{
  "name": "syncstream-backend",
  "version": "1.0.0",
  "description": "SyncStream backend server for synchronized video watching",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm install --include=dev && tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "syncstream",
    "video",
    "socket.io",
    "hls"
  ],
  "license": "MIT",
  "dependencies": {
    "@sparticuz/chromium": "^130.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "puppeteer": "^24.34.0",
    "puppeteer-core": "^23.9.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
